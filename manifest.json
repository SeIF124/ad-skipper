{
  "manifest_version": 2,
  "name": "Ad Skipper",
  "version": "1.0",
  "description": "This is an automatically youtube ads skipper extension.",
  "icons": {
    "48": "icons/48.png"
  },  
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["main.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "adskipper@seif.seif.com"
    }
  }  
}
